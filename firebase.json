{
	"hosting":{
    "headers": [ {
      "source": "**/*.@(jpg|jpeg|gif|png|js|css)",
      "headers": [ {
        "key": "Cache-Control",
        "value": "max-age=3600"
      } ]
    } ],
		"public":"dist",
		"ignore": [
			"firebase.json",
			"**/.*",
			"**/node_modules/**"
		],
		"rewrites":[
			{
				"source":"**",
				"destination":"/index.html"
			}
		]

	}
}